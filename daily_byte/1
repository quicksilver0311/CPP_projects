#include <cstdlib>
#include <iostream>
#include <iterator>
#include <algorithm>
#include <bitset>

using namespace std;

string binarySum(string a, string b) {
	int num1 = stoi(a,,2);
	int num2 = stoi(b,,2);
	string sol = bitset<32>(num1+num2).to_string();
	int indx = sol.find("1");
	cout << num1 << " " << num2 << endl;
	return sol.substr(indx);
}

int main() {
	cout << binarySum("100","1") << endl;
	cout << binarySum("11","1") << endl;
	cout << binarySum("1","0") << endl;
	return 0; 
}

